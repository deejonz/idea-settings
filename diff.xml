<application>
  <component name="TextDiffSettings">
    <option name="PLACES_MAP">
      <entry key="ApplyPatch">
        <PlaceSettings>
          <option name="SHOW_INDENT_LINES" value="true" />
          <option name="USE_SOFT_WRAPS" value="true" />
        </PlaceSettings>
      </entry>
      <entry key="CommitDialog">
        <PlaceSettings>
          <option name="EXPAND_BY_DEFAULT" value="false" />
          <option name="READ_ONLY_LOCK" value="false" />
        </PlaceSettings>
      </entry>
      <entry key="Default">
        <PlaceSettings>
          <option name="EXPAND_BY_DEFAULT" value="false" />
          <option name="IGNORE_POLICY" value="FORMATTING" />
        </PlaceSettings>
      </entry>
    </option>
  </component>
</application>